}function resetFieldValue(b){if(isSelectInputElement(b)){var a=document.getElementById(b);
a.selectedIndex=-1;
}else{document.getElementById(b).value="";
}}function LTrim(b){var a=/\s*((\S+\s*)*)/;
return b.replace(a,"$1");
}function RTrim(b){var a=/((\s*\S+)*)\s*/;
return b.replace(a,"$1");
}function trim(a){return LTrim(RTrim(a));
}function disableNextPreviousBtn(){disableEnableNextPreviousBtn(true);
}function enableNextPreviousBtn(){disableEnableNextPreviousBtn(false);
}function disableEnableNextPreviousBtn(a){var c=document.getElementById("submitNextButton");
var b=document.getElementById("submitPreviousButton");
if(c!=null){c.disabled=a;
}if(b!=null){b.disabled=a;
}if(isTriggeringControlOriginalyDisabled){triggeringControlObject.disabled=true;
}else{if(triggeringControlObject&&(triggeringControlObject!=null)){triggeringControlObject.disabled=a;
}}}function limitText(b,a){if(b.value.length>a){b.value=b.value.substring(0,a);
}}