}else{if($("#searchTypeId").val()==5){$("#SBDec").hide();
$("#SBRequestNo").hide();
$("#SBGenInfo").hide();
$("#SBVCCNo").hide();
$("#SBChassisNo").show();
}else{if($("#searchTypeId").val()==-1){$("#SBDec").hide();
$("#SBGenInfo").hide();
$("#SBRequestNo").hide();
$("#SBVCCNo").hide();
$("#SBChassisNo").hide();
}}}}}}}function isSpecialCharForCustCode(c,f){var a=true;
var d=/[+$&\\_/,:;=?@#|�>!`<~%"^} *{.]/;
var b=$(c).val();
if(d.test(b)){showVCCError("<ul><li>"+f+" cannot have special characters.</li>");
a=false;
}return a;
